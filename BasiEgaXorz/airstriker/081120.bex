main:
    cls
 	LoadTiles logo,128,256
 	LoadTiles menutiles,64,456
 	LoadTiles gametiles,128,556
	if music = 1 then
		reload musicData
    	gosub tfdPlayInit
		on VBLANK gosub tfdPlayFrame
    	enable INTERRUPTVBLANK
    endif
    strt = 1
    menuy = 0
    drawblock 256,12,5,8
    drawblock 320,20,5,8
    locate 16,5
    print "1P Hiscore - "; hiscore
    locate 22,0
	print "Copyright 2008 Electrokinesis Studios"
mainLoop:
	joy = joypad(0)
    if joy.7 then
		if music = 1 then
			reload menumusic
    		gosub tfdPlayInit
    	endif
    	goto mainmenu
    endif
    sleep 2
    locate 19,14
	if strt > 19 then 
		print "Press Start"
	else
		print "           "
	endif
	if strt < 41 then
		strt++
	else
		strt = 1
	endif
    goto mainLoop
    end
    
