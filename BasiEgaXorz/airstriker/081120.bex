game:
	cls
	if music = 1 then
		reload levelmusic
		gosub tfdPlayInit
	endif
	bugavoid = addsprite(1,1)
	propsprite bugavoid, 556, 0
	movesprite bugavoid, 20, 20
	for i = 0 to 4
		bs(i) = addsprite(1,1)
		propsprite bs(i), 566, 0
		bsx(i) = rnd(320) + 128
		bsy(i) = rnd(125)
		movesprite bs(i), bsx(i), bsy(i)
		ms(i) = addsprite(1,1)
		propsprite ms(i), 567, 0
		msx(i) = rnd(320) + 128
		msy(i) = rnd(125)
		movesprite ms(i), msx(i), msy(i)
	next
	shipx = 272
	shipy = 310
	ship = addsprite(3,3)
	propsprite ship, 556, 0
gameloop:
	joy = joypad(0)
	if joy.2 then
		if shipx > 140 then
			shipx = shipx - 5
		endif
	endif
	if joy.3 then
		if shipx < 410 then
			shipx = shipx + 5
		endif
	endif
	if joy.4 and shotb4 = 0 then
		if b = 0 then
			shotb4 = 1
			for b = 0 to 9
				if shot(b) = 0 then
					goto shoot
				endif
			next
			goto shootfail
shoot:
			shot(b) = addsprite(1,3)
			shotx(b) = shipx
			shoty(b) = shipy
			propsprite shot(b), 606, 0
			movesprite shot(b), shotx(b), shoty(b)
			b = 0
		endif
shootfail:
	endif
	if joy.4 = 0 then
		shotb4 = 0
	endif
	if joy.7 and pausedb4 = 0 then
		locate 20,5
		print "Paused, press A+B+C to quit"
		pausedb4 = 1
		joy = joypad(0)
		while 1 = 1
			joy = joypad(0)
			sleep 1
			if joy.7 = 0 then
				pausedb4 = 0
			endif
			if joy.7 and pausedb4 = 0 then
				goto unpause
			endif
			if joy.4 and joy.5 and joy.6 then
				ship = 0
				for i = 0 to 9
					enemy(i) = 0
					enemshot(i) = 0
					shot(i) = 0
				next
				for i = 0 to 4
					bs(i) = 0
					bsx(i) = 0
					bsy(i) = 0
				next
				score = 0
				freeallsprites
				goto main
			endif
		wend
unpause:
		pausedb4 = 1
		locate 20,5
		print "                            "
	endif
	if joy.7 = 0 then
		pausedb4 = 0
	endif
	for i = 0 to 4
		bsy(i) = bsy(i) + 5
		movesprite bs(i), bsx(i), bsy(i)
		msy(i) = msy(i) + 3
		movesprite ms(i), msx(i), msy(i)
		if bsy(i) > 370 then
			bsx(i) = rnd(320) + 128
			bsy(i) = rnd(125)
		endif
		if msy(i) > 370 then
			msx(i) = rnd(320) + 128
			msy(i) = rnd(125)
		endif
	next
	for i = 0 to 9
		if enemy(i) <> 0 then
			enemyy(i) = enemyy(i) + 2
			enemyx(i) = enemyx(i) + enemydeltax(i)
			movesprite enemy(i), enemyx(i), enemyy(i)
			if enemyy(i) = 180 and enemshot(i) = 0 then
				enemshot(i) = addsprite(1,1)
				enemshotx(i) = enemyx(i) + 8
				enemshoty(i) = enemyy(i) + 20
				propsprite enemshot(i), 578, 0
				movesprite enemshot(i), enemshotx(i), enemshoty(i)
			endif
			if enemyy(i) > 370 then
				freesprite enemy(i)
				enemy(i) = 0
			endif
'			if collision() = 1 then
				if enemyx(i)+24 > shipx and enemyy(i)+24 > shipy and enemyx(i) < shipx+24 and enemyy(i) < shipy+24 and explo(i) = 0 then
					goto die
				endif
'			endif
		endif
		if shot(i) <> 0 then
			shoty(i) = shoty(i) - 5
			movesprite shot(i), shotx(i), shoty(i)
			if shoty(i) < 120 then
				freesprite shot(i)
				shot(i) = 0
			endif
'			if collision() = 1 then
				for n = 0 to 9
					if enemy(n) <> 0 then
						if enemyx(n)+24 > shotx(i) and enemyy(n)+24 > shoty(i) and enemyx(n) < shotx(i)+24 and enemyy(n) < shoty(i)+8 and explo(n) = 0 then
							freesprite shot(i)
							shot(i) = 0
							explo(n) = 1
							score = score + 20
							if score > hiscore then
								hiscore = score
							endif
						endif
					endif
				next
'			endif
		endif
		if enemshot(i) <> 0 then
			enemshoty(i) = enemshoty(i) + 3
			movesprite enemshot(i), enemshotx(i), enemshoty(i)
			if enemshoty(i) > 370 then
				freesprite enemshot(i)
				enemshot(i) = 0
			endif
'			if collision() = 1 then
				if enemshotx(i)+8 > shipx and enemshoty(i)+8 > shipy and enemshotx(i) < shipx+24 and enemshoty(i) < shipy+24 then
					goto die
				endif
'			endif
		endif
		if bomb(i) <> 0 then
			bomby(i) = bomby(i) + 2
			bombx(i) = bombx(i) + bombdeltax(i)
			movesprite bomb(i), bombx(i), bomby(i)
			if bomby(i) > 370 then
				freesprite bomb(i)
				bomb(i) = 0
			endif
'			if collision() = 1 then
				if bombx(i)+16 > shipx and bomby(i)+16 > shipy and bombx(i) < shipx+24 and bomby(i) < shipy+24 then
					goto die
				endif
'			endif
		endif
		if explo(i) = 1 and explowait(i) = 0 then
			propsprite enemy(i), 579, 0
			explo(i) = 2
			explowait(i) = 10
		endif
		if explo(i) = 2 and explowait(i) = 0 then
			propsprite enemy(i), 588, 0
			explo(i) = 3
			explowait(i) = 10
		endif
		if explo(i) = 3 and explowait(i) = 0 then
			propsprite enemy(i), 597, 0
			explo(i) = 4
			explowait(i) = 10
		endif
		if explo(i) = 4 and explowait(i) = 0 then
			freesprite enemy(i)
			enemy(i) = 0
			explo(i) = 0
		endif
		if explowait(i) > 0 then
			explowait(i)--
		endif
	next
	movesprite ship, shipx, shipy
	locate 1,1
	print "Lives: "; lives
	locate 1,12
	print "Score: "; score
	locate 1,27
	print "Hi: "; hiscore
	gosub levelstuff
	sleep 1
	goto gameloop
die:
	for i = 0 to 9
		enemy(i) = 0
		enemshot(i) = 0
		explo(i) = 0
		shot(i) = 0
		bomb(i) = 0
	next
	for i = 0 to 4
		bs(i) = 0
		bsx(i) = 0
		bsy(i) = 0
		ms(i) = 0
		msx(i) = 0
		msy(i) = 0
	next
	locate 15, 15
	print "You died."
	for i = 0 to 2
		propsprite ship, 579+(i*9), 0
		sleep 10
	next
	freesprite ship
	ship = 0
	if lives > 0 then
		sleep 45
		freeallsprites
		lives--
		levdiv = 1
		levwait = 0
		goto game
	else
		locate 16,15
		print "Game over"
		sleep 75
		freeallsprites
		score = 0
		goto main
	endif

