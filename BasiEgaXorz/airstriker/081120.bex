menuloop:
	joy = joypad(0)
	if joy.0 = 0 then
		up = 0
	endif
	if joy.0 and up = 0 then
		up = 1
		if menuy > 0 then
			wait = 5
			menuy = menuy - 2
		endif
	endif
	if joy.1 = 0 then
		down = 0
	endif
	if joy.1 and down = 0 then
		down = 1
		if menuy < 6 then
			wait = 5
			menuy = menuy + 2
		endif
	endif
	if joy.1 = 0 then
		down = 0
	endif
	if joy.4 and button = 0 then
		button = 1
		if menuy = 0 then
			goto gamemenu
		elseif menuy = 2 then
			goto highscore
		elseif menuy = 4 then
			goto options
		elseif menuy = 6 then
			goto credits
		endif
	endif
	if joy.4 = 0 then
		button = 0
	endif
	drawtile 464, 10, 10+menuy
	sleep 1
	drawtile 465, 10, 10+menuy
	goto menuloop
    
