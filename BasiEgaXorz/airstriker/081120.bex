		endif
		if shot(i) <> 0 then
			shoty(i) = shoty(i) - 5
			movesprite shot(i), shotx(i), shoty(i)
			if shoty(i) < 120 then
				freesprite shot(i)
				shot(i) = 0
			endif
'			if collision() = 1 then
				for n = 0 to 9
					if enemy(n) <> 0 then
						if enemyx(n)+24 > shotx(i) and enemyy(n)+24 > shoty(i) and enemyx(n) < shotx(i)+24 and enemyy(n) < shoty(i)+8 and explo(n) = 0 then
							freesprite shot(i)
							shot(i) = 0
							explo(n) = 1
							score1p = score1p + 20
							if score1p > hicoop1 then
								hiendr1 = score1p
							endif
						endif
					endif
				next
'			endif
		endif
		if shot2(i) <> 0 then
			shot2y(i) = shot2y(i) - 5
			movesprite shot2(i), shot2x(i), shot2y(i)
			if shot2y(i) < 120 then
				freesprite shot2(i)
				shot2(i) = 0
			endif
'			if collision() = 1 then
				for n = 0 to 9
					if enemy(n) <> 0 then
