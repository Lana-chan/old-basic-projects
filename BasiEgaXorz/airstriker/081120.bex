					goto 2pcdie
				endif
'			endif
		endif
		if bomb(i) <> 0 then
			bomby(i) = bomby(i) + 2
			bombx(i) = bombx(i) + bombdeltax(i)
			movesprite bomb(i), bombx(i), bomby(i)
			if bomby(i) > 370 then
				freesprite bomb(i)
				bomb(i) = 0
			endif
'			if collision() = 1 then
				if bombx(i)+16 > shipx and bomby(i)+16 > shipy and bombx(i) < shipx+24 and bomby(i) < shipy+24 then
					goto 1pcdie
				endif
				if bombx(i)+16 > ship2x and bomby(i)+16 > ship2y and bombx(i) < ship2x+24 and bomby(i) < ship2y+24 then
					goto 2pcdie
				endif
'			endif
		endif
		if explo(i) = 1 and explowait(i) = 0 then
			propsprite enemy(i), 579, 0
			explo(i) = 2
			explowait(i) = 10
		endif
		if explo(i) = 2 and explowait(i) = 0 then
			propsprite enemy(i), 588, 0
			explo(i) = 3
			explowait(i) = 10
		endif
		if explo(i) = 3 and explowait(i) = 0 then
			propsprite enemy(i), 597, 0
			explo(i) = 4
			explowait(i) = 10
		endif
		if explo(i) = 4 and explowait(i) = 0 then
			freesprite enemy(i)
			enemy(i) = 0
			explo(i) = 0
		endif
		if explowait(i) > 0 then
			explowait(i)--
		endif
	next
	movesprite ship, shipx, shipy
	movesprite ship2, ship2x, ship2y
	locate 1,1
