endulev:
	locate 1,1
	enemcount = 0
	bombcount = 0
	spawn = rnd(10)
	which = rnd(2)
	if spawn = 1 then
		if which = 0 then
			for x = 0 to 9
				if enemy(x) <> 0 then
					enemcount++
				endif
			next
			if enemcount < 10 then
				enemx = rnd(300)+137
				enemydx = rnd(3) - 1
				gosub spawnenemy
			endif
		elseif which = 1 then
			for x = 0 to 9
				if bomb(x) <> 0 then
				bombcount++
				endif
			next
			if bombcount < 10 then
				bmbx = rnd(300)+137
				bombdx = rnd(3) - 1
				gosub spawnbomb
			endif
		endif
	endif
	return

